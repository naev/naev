msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:63
#: ../dat/events/tutorial/tutorial-combat2.lua:52
msgid "I am Iron Man!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:64
#: ../dat/events/tutorial/tutorial-combat2.lua:53
msgid "My shields are holding fine!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:65
#: ../dat/events/tutorial/tutorial-combat2.lua:54
msgid "You'd be dead if I'd remembered to pack my weapons!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:66
#: ../dat/events/tutorial/tutorial-combat2.lua:55
msgid "I'll end you!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:67
#: ../dat/events/tutorial/tutorial-combat2.lua:56
msgid "This is your last chance to surrender!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:68
#: ../dat/events/tutorial/tutorial-combat2.lua:57
msgid "I'm going to report you to the NPC Rights watchdog."
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:69
#: ../dat/events/tutorial/tutorial-combat2.lua:58
msgid "Keep going, see what happens!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:70
#: ../dat/events/tutorial/tutorial-combat2.lua:59
msgid "You don't scare me!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:71
#: ../dat/events/tutorial/tutorial-combat2.lua:60
msgid "What do you think this is, knitting hour?"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:72
#: ../dat/events/tutorial/tutorial-combat2.lua:61
msgid "Go on, shoot me."
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:73
#: ../dat/events/tutorial/tutorial-combat2.lua:62
msgid "You can do it! I believe in you."
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:74
#: ../dat/events/tutorial/tutorial-combat2.lua:63
msgid "Shoot me!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:75
#: ../dat/events/tutorial/tutorial-combat2.lua:64
msgid "Okay, listen. I'm doing this for attention."
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:76
#: ../dat/events/tutorial/tutorial-combat2.lua:65
msgid "If you don't shoot me, I'll tell the galaxy your terrible secret!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:79
#: ../dat/events/tutorial/tutorial-combat2.lua:68
msgid "Okay, that's about enough."
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:80
#: ../dat/events/tutorial/tutorial-combat2.lua:69
msgid "You can stop now."
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:81
#: ../dat/events/tutorial/tutorial-combat2.lua:70
msgid "I was wrong about you."
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:82
#: ../dat/events/tutorial/tutorial-combat2.lua:71
msgid "Forgive and forget?"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:83
#: ../dat/events/tutorial/tutorial-combat2.lua:72
msgid "Let's be pals, I'll buy you an ale!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:84
#: ../dat/events/tutorial/tutorial-combat2.lua:73
msgid "Game over, you win."
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:85
#: ../dat/events/tutorial/tutorial-combat2.lua:74
msgid "I've got a wife and kids! And a cat!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:86
#: ../dat/events/tutorial/tutorial-combat2.lua:75
msgid "Surely you must have some mercy?"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:87
#: ../dat/events/tutorial/tutorial-combat2.lua:76
msgid "Please stop!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:88
#: ../dat/events/tutorial/tutorial-combat2.lua:77
msgid "I'm sorry!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:89
#: ../dat/events/tutorial/tutorial-combat2.lua:78
msgid "Leave me alone!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:90
#: ../dat/events/tutorial/tutorial-combat2.lua:79
msgid "What did I ever do to you?"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:91
#: ../dat/events/tutorial/tutorial-combat2.lua:80
msgid "I didn't sign up for this!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:92
#: ../dat/events/tutorial/tutorial-combat2.lua:81
msgid "Not my ship, anything but my ship!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:93
#: ../dat/events/tutorial/tutorial-combat2.lua:82
msgid "We can talk this out!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:94
#: ../dat/events/tutorial/tutorial-combat2.lua:83
msgid "I'm scared! Hold me."
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:95
#: ../dat/events/tutorial/tutorial-combat2.lua:84
msgid "Make the bad ship go away, mommy!"
msgstr ""

#: ../dat/events/tutorial/tutorial-combat1.lua:96
#: ../dat/events/tutorial/tutorial-combat2.lua:85
msgid "If you don't stop I'll cry!"
