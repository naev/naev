#: dat/events/neutral/news.lua:590
msgid "{pnt} in {sys}"
msgstr "{sys}星系の{pnt}"

#: dat/events/neutral/news.lua:592
msgid "{cargo}: {price}"
msgstr "{cargo} : {price}"

