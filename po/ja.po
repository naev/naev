#: ../dat/missions/empire/longdistanceshipping/emp_longdistancecargo5.lua:12
#: ../dat/missions/empire/longdistanceshipping/emp_longdistancecargo6.lua:12
msgid ""
"Lieutenant Czesc from the Empire Aramda Shipping Division is sitting at the "
"bar."
msgstr ""

#: ../dat/missions/empire/longdistanceshipping/emp_longdistancecargo1.lua:13
#: ../dat/missions/empire/longdistanceshipping/emp_longdistancecargo1.lua:18
msgid "Soromid Long Distance Recruitment"
msgstr ""

#: ../dat/missions/empire/longdistanceshipping/emp_longdistancecargo1.lua:14
#: ../dat/missions/empire/longdistanceshipping/emp_longdistancecargo2.lua:14
#: ../dat/missions/empire/longdistanceshipping/emp_longdistancecargo3.lua:14
#: ../dat/missions/empire/longdistanceshipping/emp_longdistancecargo4.lua:14
