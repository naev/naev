#. <mission name="Diversion from Raelid">
#. <unique />
#. <priority>2</priority>
#. <chance>60</chance>
#. <done>Disrupt a Dvaered Patrol</done>
#. <location>Bar</location>
#. <faction>FLF</faction>
#. <cond>spob.cur():reputation("FLF") &gt;= 10</cond>
