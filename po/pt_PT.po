#.
#. <?xml version='1.0' encoding='utf8'?>
#. <mission name="An old woman">
#. <unique />
#. <priority>4</priority>
#. <chance>3</chance>
#. <location>Bar</location>
#. <faction>Dvaered</faction>
#. <cond>return require("misn_test").reweight_active()</cond>
