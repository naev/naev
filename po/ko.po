#: dat/missions/neutral/foodbiz/deliverlove.lua:28
msgid "Love Letters"
msgstr ""

#: dat/missions/neutral/foodbiz/deliverlove.lua:29
msgid "A cargo of feelings inked onto pulped, dried cellulose fibres."
msgstr ""

