#. <?xml version='1.0' encoding='utf8'?>
#. <mission name="Lucas 01">
#. <unique />
#. <chance>20</chance>
#. <location>Bar</location>
#. <cond>
#. -- Required to bribe Maanen's Moon
#. if spob.get("Maanen's Moon"):reputation() &lt; 0 then
