#.
#. <?xml version='1.0' encoding='utf8'?>
#. <mission name="DIY Nerds">
#. <unique />
#. <priority>4</priority>
#. <chance>2</chance>
#. <location>Bar</location>
#. <cond>
#. local mtest = require("misn_test")
#. if not mtest.cargo(true) or not mtest.reweight_active() then
