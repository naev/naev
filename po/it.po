#: dat/missions/taiomi/taiomi10.lua:232
msgid "Brace for hostiles!"
msgstr ""

#: dat/missions/taiomi/taiomi10.lua:233
msgid "We must not falter!"
msgstr ""

#: dat/missions/taiomi/taiomi10.lua:234
msgid "Almost there!"
msgstr ""

#: dat/missions/taiomi/taiomi10.lua:235
msgid "Prepare for jump!"
msgstr ""

