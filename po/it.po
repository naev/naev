msgstr ""

#. Mission details
#: dat/missions/taiomi/taiomi05.lua:79
msgid "You have been asked to help find {name}, who went missing."
msgstr ""

#: dat/missions/taiomi/taiomi05.lua:85
msgid "Land on {base}"
msgstr ""

#: dat/missions/taiomi/taiomi05.lua:86
msgid "Search for {name}"
msgstr ""

#: dat/missions/taiomi/taiomi05.lua:96 dat/missions/taiomi/taiomi05.lua:97
#: dat/missions/taiomi/taiomi05.lua:101
msgid "Search for {name} ({sys})"
msgstr ""

#: dat/missions/taiomi/taiomi05.lua:98
msgid "Scavenger must survive"
msgstr ""

#. Less preferred as a target
#. beefy shields
#: dat/missions/taiomi/taiomi05.lua:140
msgid "Our task is of uttermost importance!"
msgstr ""

#: dat/missions/taiomi/taiomi05.lua:142 dat/missions/taiomi/taiomi07.lua:187
msgid "Aaaaaagh!"
msgstr ""

#: dat/missions/taiomi/taiomi05.lua:148
msgid "Scavenger died! You were supposed to protect them!"
msgstr ""

#. Make whatever attacks scavenger hostile
#: dat/missions/taiomi/taiomi05.lua:162
msgid "I have marked the first location."
msgstr ""

#: dat/missions/taiomi/taiomi05.lua:166
