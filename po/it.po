#: dat/missions/neutral/diy-nerds.lua:127
#: dat/missions/neutral/diy-nerds.lua:152
#: dat/missions/neutral/diy-nerds.lua:180
#: dat/missions/neutral/diy-nerds.lua:185
#: dat/missions/neutral/diy-nerds.lua:248
#: dat/missions/neutral/diy-nerds.lua:258
#: dat/missions/neutral/diy-nerds.lua:271
#: dat/missions/neutral/diy-nerds.lua:311
msgid "DIY Nerds"
msgstr ""

#: dat/missions/neutral/diy-nerds.lua:118
msgid "Cart some nerds to their contest, and back."
msgstr ""

#: dat/missions/neutral/diy-nerds.lua:128
#: dat/missions/neutral/diy-nerds.lua:181
#: dat/missions/neutral/diy-nerds.lua:186
msgid "Bring the nerds and their box to {pnt} before {time}"
msgstr ""

#. Case still in time
#: dat/missions/neutral/diy-nerds.lua:129
#: dat/missions/neutral/diy-nerds.lua:182
#: dat/missions/neutral/diy-nerds.lua:274 dat/missions/shadow/shadowrun.lua:118
